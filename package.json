{
    "name": "lyst",
    "displayName": "Lyst",
    "description": "Todo list helper",
    "version": "0.1.0",
    "publisher": "morgancodes",
    "engines": {
        "vscode": "^1.29.0"
    },
    "categories": [
        "Other"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/mlaco/Lyst.git"
    },
    "activationEvents": [
        "onCommand:extension.lyst"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.lyst",
                "title": "Lyst"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "eslint": "^4.11.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
